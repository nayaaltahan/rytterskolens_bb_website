web.utils.Array={
unique:function(array,key){
var cache={},uniques=[];
if(typeof key!=='undefined'){
return array.filter(function(item){
if(cache[item[key]]){
return false
}
cache[item[key]]=true;
return true
})
}else{
return array.filter(function(item){
if(uniques.indexOf(item)>-1){
return false
}
uniques.push(item);
return true
})
}
