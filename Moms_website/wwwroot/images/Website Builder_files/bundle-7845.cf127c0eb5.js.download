web.panels.props.widgets.Widget=Ext.extend(web.panels.Panel,{
initComponent:function(){
this.cls='vbox';
this.items=[
{
ref:'form',
border:false,
xtype:'form',
cls:this.widgetClass+'flex'||'',
padding:'20px 20px 0px 20px',
labelWidth:150
},
