finalCallback.call(scope,arr)
}
}else{
itemCallback.call(scope,arr[indexUnderExecution],indexUnderExecution,function(){
web.utils.Array.asyncProcessing(arr,scope,itemCallback,finalCallback,indexUnderExecution)
})
}
}
